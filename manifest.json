{
  "manifest_version": 3,
  "name": "AdBoost",
  "version": "1.0",
  "description": "AdBoost is the only browser extension that adds ads to web pages.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
